package Paquete;

public class Ejemplos {
	
	public static void main (String[] args){
	// usos de switch y case
	char x;
	x='a';
	switch(x)
	{
	case 'a':
	{
		System.out.println("Esta es la opion 0 ");
		break;
	}
	// en case estan las opciones
	case 'b':
	{
		System.out.println("Esta es la opcion 1 ");
		break;
	}
	
	}
	}
}
