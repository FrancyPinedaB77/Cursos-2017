package Paquete;

public class Ejemplos {
	
	public static void main (String[] args){
		// Usando arreglos en Java
		// Declarando arreglos
		int y[]={10,2,4};
		int x[];
		x = new int [3];
		x[0]=10;
		x[1]=2;
		x[2]=4;
		for (int i=0;i<x.length;i++)
		{
			System.out.println("Arreglo en indice:"+i+"es igual a "+x[i]);
		}
		
	}
}
