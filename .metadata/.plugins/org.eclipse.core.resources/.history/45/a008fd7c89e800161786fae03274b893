package paquete;
import java.io.*;

public class Textos {
	try{
		FileReader r= new FileReader(nombreArchivo);//Clase usada para leer archivos de texto
		BufferedReader buffer = new BufferedReader (r);
		
		System.out.println(buffer.readLine());
	}catch(Exception e){
		System.out.println(e.getMessage());
	};

}
